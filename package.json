{
	"name": "code-zoom",
	"displayName": "Code Zoom",
	"description": "Make code small, make code big. Easy as that.",
	"version": "0.1.0",
	"publisher": "adevade",
	"repository": "https://github.com/adevade/code-zoom",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-zoom.default",
		"onCommand:code-zoom.small"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-zoom.default",
				"title": "Code Zoom: Default"
			},
			{
				"command": "code-zoom.small",
				"title": "Code Zoom: Small"
			}
		]
	},
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.32.0"
	}
}
