{
	"name": "code-zoom",
	"displayName": "Code Zoom",
	"description": "Make code small, make code big.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-zoom.default",
		"onCommand:code-zoom.small"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-zoom.default",
				"title": "Code Zoom: Default"
			},
			{
				"command": "code-zoom.small",
				"title": "Code Zoom: Small"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
